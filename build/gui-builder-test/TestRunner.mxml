<?xml version="1.0" encoding="utf-8"?>
<s:Application 
xmlns:fx="http://ns.adobe.com/mxml/2009"
xmlns:s="library://ns.adobe.com/flex/spark" 
creationComplete="runTests();">
   
   <fx:Script>
      <![CDATA[
         import org.flexunit.listeners.CIListener;
         import org.flexunit.runner.FlexUnitCore;
         import org.flexunit.runner.Request;
         
import lse.math.games.builder.model.ExtensiveFormTest;
import lse.math.games.builder.model.IsetTest;
import lse.math.games.builder.model.MoveTest;
import lse.math.games.builder.model.NodeTest;
import lse.math.games.builder.model.OutcomeTest;
import lse.math.games.builder.model.PlayerTest;
import lse.math.games.builder.model.RationalTest;
import lse.math.games.builder.model.StrategyTest;

         
         public function runTests() : void {
            var core : FlexUnitCore = new FlexUnitCore();
            core.addListener(new CIListener());
            
			var request:Request = Request.qualifyClasses.apply(
					null, 
[ExtensiveFormTest,IsetTest,MoveTest,NodeTest,OutcomeTest,PlayerTest,RationalTest,StrategyTest]
				);
            core.run(request);
         }
      ]]>
   </fx:Script>
</s:Application>
